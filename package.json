{
  "name": "@codexcentral/generate-uuid",
  "version": "1.1.0",
  "description": "This is a simple package to generate a UUID (Universally Unique Identifier).",
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": ["typescript", "uuid", "generate", "generate-uuid"],
  "author": "Roberto Silva Zu√±iga <c.roberto.u@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Codex-Central/generate-uuid.git"
  },
  "bugs": {
    "url": "https://github.com/Codex-Central/generate-uuid/issues"
  },
  "homepage": "https://github.com/Codex-Central/generate-uuid#readme",
  "scripts": {
    "build": "rm -rf dist/ && npm run build:esm && npm run build:cjs",
